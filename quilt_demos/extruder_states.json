{
        "name": "extruder_states",
        "uuid": "xxxxxxxxxxxxxxx",
        "state_definitions" : [
            {
               "label" : "PRODUCING",
               "expression" : [
                 ["machine_power", "==", 1],
                 ["melt_temperature", ">=", 115],
                 ["screw_speed", ">=", 10],
                 ["line_speed", ">=", 15]
               ],
               "expression_operator" : "AND"
            },
            {
               "label" : "IDLING",
               "expression" : [
                 ["machine_power", "==", 1],
                 ["melt_temperature", "<", 115],
                 ["screw_speed", ">=", 0],
                 ["line_speed", ">=", 0]
               ],
               "expression_operator" : "AND"
            },
            {
               "label" : "PURGING",
               "expression" : [
                 ["machine_power", "==", 1],
                 ["melt_temperature", "<", 115],
                 ["screw_speed", ">", 10],
                 ["line_speed", ">", 0]
               ],
               "expression_operator" : "AND"
            },
            {
               "label" : "POWER_OFF",
               "expression" : [
                 ["machine_power", "<", 1],
                 ["melt_temperature", "<=", 200]
               ],
               "expression_operator" : "AND"
            }
        ],
        "input_timeseries" : [
